{% extends 'website/base.html.twig' %}

{% block title %}{{ page.title }}{% endblock %}

{% block body %}
    <div class="page-header" style="background-image: url({{ asset('img/swimmer_color.jpg') }})">
        <div class="text-position">
            <h1>{{ page.title }}</h1>
        </div>
    </div>

    <div class="page-content">
        {{ page.content|raw }}
    </div>
{% endblock %}